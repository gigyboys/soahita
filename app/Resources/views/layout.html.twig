<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<title>{% block title %}Soahita{% endblock %}</title>
		<link rel="icon" type="image/png" href="{{ asset('asset/image/logo30x30.png') }}" />
		<meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
		<link rel="stylesheet" href="{{ asset('asset/bower_components/bootstrap/dist/css/bootstrap.min.css') }}">
		<link rel="stylesheet" href="{{ asset('asset/bower_components/font-awesome/css/font-awesome.min.css') }}">
		<link rel="stylesheet" href="{{ asset('asset/bower_components/Ionicons/css/ionicons.min.css') }}">
		<link rel="stylesheet" href="{{ asset('asset/dist/css/AdminLTE.min.css') }}">
		<link rel="stylesheet" href="{{ asset('asset/dist/css/skins/_all-skins.min.css') }}">
		<link rel="stylesheet" href="{{ asset('asset/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css') }}">
		<link rel="stylesheet" href="{{ asset('asset/bower_components/datatables.net-bs/css/dataTables.bootstrap.min.css') }}">
		<link rel="stylesheet" href="{{ asset('asset/dist/css/custom.css') }}">
		<link rel="stylesheet" href="{{ asset('asset/css/popup.css') }}">
		<link rel="stylesheet" href="{{ asset('asset/css/style.css') }}">
		<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic" />
		<link rel="stylesheet" href="{{ asset('asset/bower_components/jquery-ui/themes/cupertino/jquery-ui.css') }}">
		<link rel="stylesheet" href="{{ asset('asset/bower_components/jquery-ui/themes/jquery-ui-timepicker-addon.css') }}">
	</head>
	
	{% set collapse = ''%}
	{% if app.session.get('dataCollapse').collapse is defined %}
		{% if app.session.get('dataCollapse').collapse %}
			{% set collapse = 'sidebar-collapse'%}
		{% endif %}
	{% endif %}
	
	<body class="hold-transition skin-blue sidebar-mini {{ collapse }} ">
		<script type="text/javascript">
			 var urlLangDataTableFR = '{{ asset('asset/i18n/dataTables.french.lang') }}';
		</script>
		<div class="wrapper">
			{{ include("::header.html.twig") }}
			{{ include("::sidebar.html.twig") }}
			<div class="content-wrapper">
				{% block body %}
				Soahita
				{% endblock %}
			</div>
			{{ include("::footer.html.twig") }}
		</div>

		<script src="{{ asset('asset/bower_components/jquery/dist/jquery.min.js') }}"></script>
		<script src="{{ asset('asset/bower_components/bootstrap/dist/js/bootstrap.min.js') }}"></script>
		
		<script src="{{ asset('asset/bower_components/jquery-ui/jquery-ui.js') }}"></script>
		<script src="{{ asset('asset/bower_components/jquery-ui/jquery-ui-timepicker-addon.js') }}"></script>
		
		<script src="{{ asset('asset/plugins/input-mask/jquery.inputmask.js') }}"></script>
		<script src="{{ asset('asset/plugins/input-mask/jquery.inputmask.date.extensions.js') }}"></script>
		<script src="{{ asset('asset/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js') }}"></script>
		<script src="{{ asset('asset/plugins/bootstrap-wysihtml5/i18n/bootstrap-wysihtml5.fr-FR.js') }}"></script>
		<script src="{{ asset('asset/bower_components/datatables.net/js/jquery.dataTables.min.js') }}"></script>
		<script src="{{ asset('asset/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js') }}"></script>
		<script src="{{ asset('asset/bower_components/Chart.js/Chart.js') }}"></script>
		<script src="{{ asset('asset/dist/js/adminlte.min.js') }}"></script>
		<script src="{{ asset('asset/js/common.js') }}"></script>
		<script src="{{ asset('asset/js.js') }}"></script>
		<script src="{{ asset('asset/js/popup.js') }}"></script>
		<script src="{{ asset('asset/js/bs.js') }}"></script>
	</body>
</html>